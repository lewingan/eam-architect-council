# Anthropic API key (required for live mode)
ANTHROPIC_API_KEY=sk-ant-...

# Model to use (optional, defaults to claude-sonnet-4-20250514)
EAM_MODEL=claude-sonnet-4-20250514
# Optional cost controls
# Global estimated token budget per minute across all Anthropic requests
EAM_TOKENS_PER_MINUTE=30000
# Set to 0/false to disable request throttling
EAM_ENABLE_TPM_THROTTLE=true
